{
    "name": "dorcas/modules-finance",
    "description": "Dorcas Hub Finance Module",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Ifeoluwa Olawoye",
            "email": "bolaji.olawoye@hostvillenigeria.org"
        }
    ],
    "require": {
        "php": ">=7.4",
        "aws/aws-sdk-php": "^3.74",
        "barryvdh/laravel-dompdf": "^1.0.2",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.24"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Dorcas\\ModulesFinance\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dorcas\\ModulesFinance\\ModulesFinanceServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
